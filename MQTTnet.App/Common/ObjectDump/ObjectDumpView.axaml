<UserControl
    x:Class="MQTTnet.App.Common.ObjectDump.ObjectDumpView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <ScrollViewer>
        <ItemsPresenter Grid.IsSharedSizeScope="True" Items="{Binding Properties}" />
    </ScrollViewer>
</UserControl>
